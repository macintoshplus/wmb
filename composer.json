{
    "name": "jbnahan/wmb",
    "description": "Avanced workflow manager",
    "license": "MIT",
    "authors": [
        {
            "name": "Jean-Baptiste Nahan",
            "email": "jbnahan@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "symfony/framework-bundle": ">=2.7",
        "symfony/swiftmailer-bundle": "2.3.*",
        "doctrine/orm": "~2.2,>=2.2.3,<2.5",
        "doctrine/doctrine-bundle": "~1.4",
        "twig/extensions": "~1.0",
        "ramsey/uuid": "~2.8.0"
    },
    "suggest":{
        "jms/serializer-bundle": "All entity is configured for use JMS Serializer"
    },
    "require-dev": {
        "atoum/atoum": "master-dev",
        "atoum/atoum-bundle": "1.1.*@dev"
    },
    "autoload": {
        "psr-0": { "JbNahan\\Bundle\\WorkflowManagerBundle": "" }
    },
    "target-dir": "JbNahan/Bundle/WorkflowManagerBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.x-dev",
            "dev-v1.2": "1.2.x-dev"
        }
    }
}
